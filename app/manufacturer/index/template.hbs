{{outlet}}

<div class="container">
  <div class="panel panel-default">
    <div class="panel-heading"><h4>Manufacturers</h4></div>
    <div class="panel-body">
      <table class="table table-striped">
        <thead class="table-head">
          <tr>
            <td>Manufacturer</td>
            <td>Country</td>
            <td># of Models</td>
            <td></td>
          </tr>
        </thead>
        <tbody>
          {{#each model as |project|}}
            <tr>
              <td>{{project.name}}</td>
              <td>{{project.country}}</td>
              <td>0</td>
              <td>{{#link-to 'manufacturer.index' }}See More{{/link-to}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
